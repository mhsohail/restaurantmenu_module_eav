				( ! ) Fatal error: Class 'Mage_Marketplace_Helper_Data' not found in E:\wamp\www\clikaroo\app\Mage.php on line 547
			-->
			<!--
			<action method="setTitle" translate="title" module="marketplace">
                <title>Advertising</title>
            </action>
			-->
        </reference>
		<reference name="content">
			<!-- <block type="restaurant/navbar" name="restaurant_nav" template="/restaurant/nav1.phtml"/> -->
			
			<!--
				<block type='restaurant/menu'> means, the block must be defined in etc/config.xml file.
				also, there should be Menu.php class in Block folder
			-->
			<block type="restaurant/menu" name="restaurant_menu" template="restaurant_menu/menu/menu.phtml" />
			<!--    <block type="restaurant/collection" name="restaurant_collection" template="restaurant/collection.phtml"/>-->
